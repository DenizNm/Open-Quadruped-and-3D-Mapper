<mujoco model="qrpfra">
  <compiler angle="degree" coordinate="local" inertiafromgeom="true" />
  <option integrator="RK4" timestep="0.02" gravity="0 0 -9.81" density="0.5"/>

  <default>
    <joint armature="1" damping="1" limited="true"/>
    <geom conaffinity="0" condim="3" density="5.0" friction="1 0.5 0.5" margin="0.01" rgba="0.8 0.6 0.4 1"/>
  </default>

  <asset>
    <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>
    <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0 0 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
    <texture builtin="checker"  name="texplane" rgb1="0.02 0.02 0.12" rgb2="0.98 0.98 0.88" type="2d" width="1000" height="1000"/>
    <material name="MatPlane" reflectance="0.8" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
    <material name="geom" texture="texgeom" texuniform="true"/>
    <mesh name="base_link" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/base_link.stl"/>
    <mesh name="front_Left_Hind_Limb" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/front_Left_Hind_Limb.stl"/>
    <mesh name="front_Left_MidLimb" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/front_Left_MidLimb.stl"/>
    <mesh name="front_Left_Wrist" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/front_Left_Wrist.stl"/>
    <mesh name="front_Right_Hind_Limb" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/front_Right_Hind_Limb.stl"/>
    <mesh name="front_Right_MidLimb" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/front_Right_MidLimb.stl"/>
    <mesh name="front_Right_Wrist" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/front_Right_Wrist.stl"/>
    <mesh name="rear_Left_Hind_Limb" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/rear_Left_Hind_Limb.stl"/>
    <mesh name="rear_Left_MidLimb" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/rear_Left_MidLimb.stl"/>
    <mesh name="rear_Left_Wrist" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/rear_Left_Wrist.stl"/>
    <mesh name="rear_Right_Hind_Limb" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/rear_Right_Hind_Limb.stl"/>
    <mesh name="rear_Right_MidLimb" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/rear_Right_MidLimb.stl"/>
    <mesh name="rear_Right_Wrist" file="/Users/deniz/PycharmProjects/mujoco_reinforcement_research/QRPfRA_deneme/Assets/rear_Right_Wrist.stl"/>
    <!--mesh name="scanned_stl" file="/Users/deniz/Desktop/deneme4.stl"/-->
  </asset>

  <worldbody>
    <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
    <geom conaffinity="1" condim="3" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="40 40 40" type="plane"/>
    <!--geom type="mesh" mesh="scanned_stl" pos="0 0 0.3"/-->

    <body name="base_link" pos="0 0 0.07">
      <camera name="track" mode="trackcom" pos="0 -3 0.3" xyaxes="1 0 0 0 0 1"/>
      <geom type="mesh" mesh="base_link" mass="2"/>
      <joint armature="0" damping="0" limited="false" margin="0.01" name="root" pos="0 0 0" type="free"/>
      <site name="base_site" type="box" size="0.05 0.15 0.01"  pos="0.0 0.04 0.165" euler="180 0 0"/>
      <site name="base_site_front" type="box" size="0.001 0.001 0.005"  pos="0.0 0.22 0.165" rgba="0.1 0.2 0.5 1" euler="-90 0 0"/>
      <site name="base_site_rear" type="box" size="0.001 0.001 0.005"  pos="0.0 -0.16 0.165" rgba="0.1 0.2 0.5 1" euler="90 0 0"/>

      <body name="front_Left_Hind_Limb" pos="0 0 0">
        <joint axis="0 1 0" name="FL_hind_to_base" pos="-0.04 0.2 0.18" range="-45 45" type="hinge" damping="90"/>
        <geom type="mesh" mesh="front_Left_Hind_Limb" mass="0.8"/>
        <body name="front_Left_MidLimb" pos="0 0 0">
          <joint axis="1 0 0" name="FL_mid_to_base" pos="-0.08 0.18 0.17" range="-90 90" type="hinge" damping="90" ref="-45"/>
          <geom type="mesh" mesh="front_Left_MidLimb" mass="1"/>
          <body name="front_Left_Wrist" pos="0 0 0">
            <joint axis="1 0 0" name="FL_wrist_to_mid" pos="-0.08 0.113 0.083" range="-90 90" type="hinge" damping="90"/>
            <geom type="mesh" mesh="front_Left_Wrist" mass="1"/>
            <site name="FL_foot" type="sphere" size="0.025" pos="-0.1 0.198 0.0" euler="135 0 0"/>
          </body>
        </body>
      </body>

      <body name="front_Right_Hind_Limb" pos="0 0 0">
        <joint axis="0 1 0" name="FR_hind_to_base" pos="0.04 0.2 0.18" range="-45 45" type="hinge" damping="90"/>
        <geom type="mesh" mesh="front_Right_Hind_Limb" mass="0.8"/>
        <body name="front_Right_MidLimb" pos="0 0 0">
          <joint axis="-1 0 0" name="FR_mid_to_base" pos="0.08 0.18 0.17" range="-90 90" type="hinge" damping="90" />
          <geom type="mesh" mesh="front_Right_MidLimb" mass="1"/>
          <body name="front_Right_Wrist" pos="0 0 0">
            <joint axis="-1 0 0" name="FR_wrist_to_mid" pos="0.08 0.117 0.083" range="-90 90" type="hinge" damping="90"/>
            <geom type="mesh" mesh="front_Right_Wrist" mass="1"/>
            <site name="FR_foot" type="sphere" size="0.025" pos="0.1 0.2 0.0" euler="135 0 0"/>
          </body>
        </body>
      </body>

      <body name="rear_Left_Hind_Limb" pos="0 0 0">
        <joint axis="0 1 0" name="RL_hind_to_base" pos="-0.04 -0.1 0.18" range="-45 45" type="hinge" damping="90"/>
        <geom type="mesh" mesh="rear_Left_Hind_Limb" mass="0.8"/>
        <body name="rear_Left_MidLimb" pos="0 0 0">
          <joint axis="1 0 0" name="RL_mid_to_base" pos="-0.08 -0.13 0.17" range="-90 90" type="hinge" damping="90" />
          <geom type="mesh" mesh="rear_Left_MidLimb" mass="1"/>
          <body name="rear_Left_Wrist" pos="0 0 0">
            <joint axis="1 0 0" name="RL_wrist_to_mid" pos="-0.08 -0.195 0.083" range="-90 90" type="hinge" damping="90"/>
            <geom type="mesh" mesh="rear_Left_Wrist" mass="1"/>
            <site name="RL_foot" type="sphere" size="0.025" pos="-0.1 -0.112 0.0" euler="135 0 0"/>
          </body>
        </body>
      </body>

      <body name="rear_Right_Hind_Limb" pos="0 0 0">
        <joint axis="0 1 0" name="RR_hind_to_base" pos="0.04 -0.1 0.18" range="-45 45" type="hinge" damping="90"/>
        <geom type="mesh" mesh="rear_Right_Hind_Limb" mass="0.8"/>
        <body name="rear_Right_MidLimb" pos="0 0 0">
          <joint axis="-1 0 0" name="RR_mid_to_base" pos="0.08 -0.125 0.17" range="-90 90" type="hinge" damping="90" />
          <geom type="mesh" mesh="rear_Right_MidLimb" mass="1"/>
          <body name="rear_Right_Wrist" pos="0 0 0">
            <joint axis="-1 0 0" name="RR_wrist_to_mid" pos="0.08 -0.188 0.083" range="-90 90" type="hinge" damping="90"/>
            <geom type="mesh" mesh="rear_Right_Wrist" mass="1"/>
            <site name="RR_foot" type="sphere" size="0.025" pos="0.1 -0.105 0.0" euler="135 0 0"/>
          </body>
        </body>
      </body>

    </body>
  </worldbody>
  <actuator>
    <motor name="FL_hind_to_base_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="FL_hind_to_base" gear="20" />
    <motor name="FR_hind_to_base_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="FR_hind_to_base" gear="20" />
    <motor name="RL_hind_to_base_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="RL_hind_to_base"  gear="20" />
    <motor name="RR_wrist_to_mid_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="RR_wrist_to_mid"  gear="20" />

    <motor name="FL_mid_to_base_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="FL_mid_to_base"  gear="20" />
    <motor name="FL_wrist_to_mid_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="FL_wrist_to_mid"  gear="20" />

    <motor name="FR_mid_to_base_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="FR_mid_to_base"  gear="20" />
    <motor name="FR_wrist_to_mid_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="FR_wrist_to_mid"  gear="20" />

    <motor name="RL_mid_to_base_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="RL_mid_to_base"  gear="20" />
    <motor name="RL_wrist_to_mid_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="RL_wrist_to_mid"  gear="20" />

    <motor name="RR_hind_to_base_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="RR_hind_to_base"  gear="20" />
    <motor name="RR_mid_to_base_pos_motor" ctrllimited="true" ctrlrange="-50 50" joint="RR_mid_to_base"  gear="20" />
  </actuator>
  <sensor>
    <accelerometer name="base_acc" site="base_site"/>
    <gyro name="base_gyro" site="base_site"/>

    <accelerometer name="FL_foot_acc" site="FL_foot"/>
    <gyro name="FL_foot_gyro" site="FL_foot"/>
    <touch name="FL_foot_touch" site="FL_foot"/> <!--4-->

    <accelerometer name="FR_foot_acc" site="FR_foot"/>
    <gyro name="FR_foot_gyro" site="FR_foot"/>
    <touch name="FR_foot_touch" site="FL_foot"/> <!--7-->

    <accelerometer name="RL_foot_acc" site="RL_foot"/>
    <gyro name="RL_foot_gyro" site="RL_foot"/>
    <touch name="RL_foot_touch" site="FL_foot"/> <!--10-->

    <accelerometer name="RR_foot_acc" site="RR_foot"/>
    <gyro name="RR_foot_gyro" site="RR_foot"/>
    <touch name="RR_foot_touch" site="FL_foot"/> <!--13-->

    <rangefinder name="base_range_to_ground" site="base_site"/>
    <rangefinder name="base_range_to_front" site="base_site_front"/>
    <rangefinder name="base_range_to_rear" site="base_site_rear"/>

    <jointpos name="FL_hind_to_base_angle" joint="FL_hind_to_base"/>
    <jointpos name="FR_hind_to_base_angle" joint="FR_hind_to_base"/>
    <jointpos name="RL_hind_to_base_angle" joint="RL_hind_to_base"/>
    <jointpos name="RR_wrist_to_mid_angle" joint="RR_wrist_to_mid"/>

    <jointpos name="FL_mid_to_base_angle" joint="FL_mid_to_base"/>
    <jointpos name="FL_wrist_to_mid_angle" joint="FL_wrist_to_mid"/>

    <jointpos name="FR_mid_to_base_angle" joint="FR_mid_to_base"/>
    <jointpos name="FR_wrist_to_mid_angle" joint="FR_wrist_to_mid"/>

    <jointpos name="RL_mid_to_base_angle" joint="RL_mid_to_base"/>
    <jointpos name="RL_wrist_to_mid_angle" joint="RL_wrist_to_mid"/>

    <jointpos name="RR_hind_to_base_angle" joint="RR_hind_to_base"/>
    <jointpos name="RR_mid_to_base_angle" joint="RR_mid_to_base"/>
  </sensor>
</mujoco>
